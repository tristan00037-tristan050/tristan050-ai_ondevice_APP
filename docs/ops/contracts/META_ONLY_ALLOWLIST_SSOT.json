{
  "schema_name": "META_ONLY_ALLOWLIST_V1",
  "allowed_keys": [
    "status",
    "reason_code",
    "ts_utc",
    "duration_ms_bucket",
    "request_id",
    "app_version",
    "model_version",
    "policy_version",
    "repo_sha",
    "count_*",
    "device_id_hash",
    "mode",
    "shadow",
    "runtime_ok",
    "runtime_status",
    "runtime_latency_ms_bucket",
    "runtime_manifest_sha256"
  ],
  "forbidden_patterns": [
    "patch",
    "diff",
    "commit message",
    "stack trace",
    "BEGIN",
    "END"
  ],
  "max_string_len": 120
}

