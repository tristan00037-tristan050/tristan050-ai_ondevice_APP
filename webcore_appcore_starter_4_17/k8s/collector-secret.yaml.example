apiVersion: v1
kind: Secret
metadata:
  name: collector-secrets
type: Opaque
stringData:
  db-host: "postgres-service"
  db-port: "5432"
  db-name: "collector"
  db-user: "postgres"
  db-password: "your-db-password"
  api-keys: "default:collector-key,teamA:teamA-key"
  export-sign-secret: "your-sign-secret"
  encryption-key: "your-encryption-key"

